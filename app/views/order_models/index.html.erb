<p id="notice"><%= notice %></p>

<h1>Listing Order Models</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Model</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_models.each do |order_model| %>
      <tr>
        <td><%= order_model.order_id %></td>
        <td><%= order_model.model_id %></td>
        <td><%= link_to 'Show', order_model %></td>
        <td><%= link_to 'Edit', edit_order_model_path(order_model) %></td>
        <td><%= link_to 'Destroy', order_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order model', new_order_model_path %>
