<p id="notice"><%= notice %></p>

<h1>Listing Order Services</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_services.each do |order_service| %>
      <tr>
        <td><%= order_service.order_id %></td>
        <td><%= order_service.service_id %></td>
        <td><%= link_to 'Show', order_service %></td>
        <td><%= link_to 'Edit', edit_order_service_path(order_service) %></td>
        <td><%= link_to 'Destroy', order_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order service', new_order_service_path %>
